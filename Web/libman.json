{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "simpledotcss@2.3.1",
      "destination": "wwwroot/lib/simpledotcss/",
      "files": [
        "simple.css",
        "simple.min.css"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "alpinejs@3.14.9",
      "destination": "wwwroot/lib/alpinejs/",
      "files": [
        "dist/cdn.js",
        "dist/cdn.min.js"
      ]
    }
  ]
}